<?php
use App\Days\Day$NUMBER$;

beforeEach(function (){
    $this->input = '';
    $this->day = new Day$NUMBER$(collect(Str::of($this->input)->explode("\n")));

});

test('Day $NUMBER$: Part 1', function (){
    $this->assertEquals(1, $this->day->part1());
});

test('Day $NUMBER$: Part 2', function (){
    $this->assertEquals(2, $this->day->part2());
});
